<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <!-- Meta, title, CSS, favicons, etc. -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@ViewBag.Title- Business Management System</title>
    <!-- Bootstrap -->
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <!-- Font Awesome -->
    <link href="~/Content/font-awesome.min.css" rel="stylesheet" />
    <script src="~/Content/vendors/nprogress/nprogress.js"></script>
    <link href="~/Content/green.css" rel="stylesheet" />
    <link href="~/Content/bootstrap-progressbar-3.3.4.css" rel="stylesheet" />
    <link href="~/Content/jqvmap.min.css" rel="stylesheet" />
    <link href="~/Content/daterangepicker.css" rel="stylesheet" />
    <link href="~/Content/custom.min.css" rel="stylesheet" />
</head>

<body class="nav-md">
    <div class="container body">
        <div class="main_container">
            <div class="col-md-3 left_col">
                <div class="left_col scroll-view">
                    <div class="navbar nav_title" style="border: 0;">
                        <a href="/Home/Index" class="site_title"><i class="glyphicon glyphicon-usd"></i> <span>Points Of Sales!</span></a>
                    </div>

                    <div class="clearfix"></div>

                    <!-- menu profile quick info -->
                    <div class="profile clearfix">
                        <div class="profile_pic">

                            <img alt="Business Title" class="img-circle profile_img" src="~/Resourses/Inventory.jpg" />
                        </div>
                        <div class="profile_info">
                            <span>Welcome,</span>
                            <h4>Business Management System</h4>
                        </div>
                    </div>
                    <!-- /menu profile quick info -->

                    <br />

                    <!-- sidebar menu -->
                    <div id="sidebar-menu" class="main_menu_side hidden-print main_menu">
                        <div class="menu_section">
                            <ul class="nav side-menu">
                                <li>
                                    <a><i class="glyphicon glyphicon-hand-right"></i> Product Catalog <span class="fa fa-chevron-down"></span></a>
                                    <ul class="nav child_menu">
                                        <li><a href="/Category/Index">Product Category Setup</a></li>
                                        <li><a href="#">Product Setup with Category</a></li>
                                    </ul>
                                </li>
                                <li>
                                    <a><i class="glyphicon glyphicon-hand-right"></i> Party Module<span class="fa fa-chevron-down"></span></a>
                                    <ul class="nav child_menu">
                                        <li><a href="@Url.Action("Insert")">Customer Setup </a></li>
                                        <li><a href="/Supplier/Show">Supplier Setup</a></li>
                                    </ul>
                                </li>
                                <li>
                                    <a><i class="glyphicon glyphicon-hand-right"></i> Purchase Module<span class="fa fa-chevron-down"></span></a>
                                    <ul class="nav child_menu">
                                        <li><a href="#"> Purchase Entry </a></li>
                                        <li><a href="#">Purchase Edit</a></li>
                                        <li><a href="#">Purchase Confirm</a></li>
                                        <li><a href="#">Purchase Search </a></li>
                                    </ul>
                                </li>
                                <li>
                                    <a><i class="glyphicon glyphicon-hand-right"></i> Sales Module <span class="fa fa-chevron-down"></span></a>
                                    <ul class="nav child_menu">
                                        <li><a href="#"> Sales Entry </a></li>
                                        <li><a href="#">Sales Edit</a></li>
                                        <li><a href="#">Sales Confirm</a></li>
                                        <li><a href="#">Sales Search </a></li>
                                    </ul>
                                </li>
                                <li>
                                    <a><i class="glyphicon glyphicon-hand-right"></i> Stock Module <span class="fa fa-chevron-down"></span></a>
                                    <ul class="nav child_menu">
                                        <li><a href="#">Periodical Stock Report</a></li>
                                        <li><a href="#">Find Low product & Expired product</a></li>
                                    </ul>
                                </li>
                                <li>
                                    <a><i class="glyphicon glyphicon-hand-right"></i> Reporting Module <span class="fa fa-chevron-down"></span></a>
                                    <ul class="nav child_menu">
                                        <li><a href="#">Periodic Income Expense Report on Sales </a></li>
                                        <li><a href="#">Periodic Income Expense Report on Purchase </a></li>
                                    </ul>
                                </li>
                            </ul>
                        </div>
                    </div>
                    <!-- /sidebar menu -->
                </div>
            </div>

            <!-- top navigation -->
            <div class="top_nav">
                <div class="nav_menu">
                    <nav>
                        <div class="nav toggle">
                            <a id="menu_toggle"><i class="glyphicon glyphicon-step-forward"></i></a>
                        </div>

                        <ul class="nav navbar-nav navbar-right">
                            <li class="">
                                <a href="/Home/Index"><i class="glyphicon glyphicon-home"></i> Home</a>
                            </li>
                        </ul>
                    </nav>
                </div>
            </div>
            <!-- /top navigation -->
            <!-- page content -->
            <div class="right_col" role="main">
                <div class="">
                    @*<div class="page-title">
                            <div class="title_left">
                                <h3>Form Validation</h3>
                            </div>

                            <div class="title_right">
                                <div class="col-md-5 col-sm-5 col-xs-12 form-group pull-right top_search">
                                    <div class="input-group">
                                        <input type="text" class="form-control" placeholder="Search for...">
                                        <span class="input-group-btn">
                                            <button class="btn btn-default" type="button">Go!</button>
                                        </span>
                                    </div>
                                </div>
                            </div>
                        </div>*@
                    <div class="clearfix"></div>
                    <div class="row">
                        <div class="col-md-12 col-sm-12 col-xs-12">
                            <div class="x_panel">
                                <div class="x_content">
                                    <form class="form-horizontal form-label-left" novalidate action="" method="post" enctype="multipart/form-data">
                                        @RenderBody()
                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- /page content -->
            <!-- footer content -->
            <footer>
                <div class="pull-right">
                    &copy; Copyright 2019 'GLAM SQUAD'
                </div>
                <div class="clearfix"></div>
            </footer>
            <!-- /footer content -->
        </div>
    </div>

    <!-- jQuery -->
    @*<script src="~/Scripts/jquery-3.0.0.min.js"></script>*@
    <script src="~/Scripts/jquery.min.js"></script>
    <!-- Bootstrap -->
    <script src="~/Scripts/bootstrap.min.js"></script>
    <!-- FastClick -->
    <script src="~/Scripts/fastclick.js"></script>
    <!-- NProgress -->
    <script src="~/Scripts/nprogress.js"></script>
    <!-- validator -->
    <script src="~/Scripts/validator.js"></script>
    <!-- Custom Theme Scripts -->
    <script src="~/Scripts/custom.min.js"></script>
    <script src="~/Scripts/bootstrap.bundle.min.js"></script>
    @RenderSection("scripts", required: false)

</body>
</html>